{
    "manifest_version": 3,
    "name": "MyOutfitAi",
    "version": "1.0",
    "permissions": ["storage", "contextMenus"],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  